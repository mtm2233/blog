.search{padding:35rpx}.search .search_input input{-webkit-appearance:none;background-color:#fff;background-image:none;border-radius:10rpx;border:1rpx solid #dcdfe6;box-sizing:border-box;color:#606266;display:inline-block;font-size:inherit;height:60rpx;line-height:60rpx;outline:0;padding:0 25rpx;-webkit-transition:border-color .2s cubic-bezier(.645,.045,.355,1);transition:border-color .2s cubic-bezier(.645,.045,.355,1);width:100%}.search .search_tags{margin-top:35rpx;box-sizing:border-box;padding:15rpx;border:1rpx solid #ebeef5;border-radius:15rpx;box-shadow:0 5rpx 15rpx 0 rgba(0,0,0,.1)}.search .search_tags ._span{background-color:#ecf5ff;display:inline-block;height:55rpx;padding:0 20rpx;line-height:55rpx;font-size:25rpx;color:#409eff;border:1rpx solid #d9ecff;border-radius:6rpx;box-sizing:border-box;white-space:nowrap;margin:10rpx}.search .search_tags ._span.success{background-color:#f0f9eb;border-color:#e1f3d8;color:#67c23a}.search .search_tags ._span.info{background-color:#f4f4f5;border-color:#e9e9eb;color:#909399}.search .search_tags ._span.warning{background-color:#fdf6ec;border-color:#faecd8;color:#e6a23c}.search .search_tags ._span.danger{background-color:#fef0f0;border-color:#fde2e2;color:#f56c6c}